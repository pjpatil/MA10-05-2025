[32m+[m[32mapp.secret_key = '9f2a1b8c4d6e7f0123456789abcdef01'[m
[32m+[m
[32m+[m[32mstr_pattern_reg = "^[A-Za-z ]+$"[m
[32m+[m
[32m+[m[32mclass ResponseHandler:[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def invalid_name(entity):[m
[32m+[m[32m        return {'status': 'error', 'message': f'Invalid {entity} name. Only letters are allowed!'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def already_exists(entity):[m
[32m+[m[32m        return {'status': 'exists', 'message': f'{entity.capitalize()} already exists!'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def add_success(entity):[m
[32m+[m[32m        return {'status': 'success', 'message': f'{entity.capitalize()} added successfully!'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def add_failure(entity):[m
[32m+[m[32m        return {'status': 'error', 'message': f'Failed to add {entity}.'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def is_available(entity):[m
[32m+[m[32m        return {'status': 'available', 'message': f'{entity.capitalize()} name is available!'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def delete_success(entity):[m
[32m+[m[32m        return {'status': 'success', 'message': f'{entity.capitalize()} deleted successfully!'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def delete_failure(entity):[m
[32m+[m[32m        return {'status': 'error', 'message': f'Failed to delete {entity}.'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def update_success(entity):[m
[32m+[m[32m        return {'status': 'success', 'message': f'{entity.capitalize()} updated successfully!'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def update_failure(entity):[m
[32m+[m[32m        return {'status': 'error', 'message': f'Failed to update {entity}.'}[m
[32m+[m
[32m+[m[32m    @staticmethod[m
[32m+[m[32m    def fetch_failure(entity):[m
[32m+[m[32m        return f"Failed to fetch {entity}"[m
[32m+[m
[32m+[m
[32m+[m[32m# Helper: JSON Response Formatter[m
[32m+[m[32mdef json_response(message_obj, status_code):[m
[32m+[m[32m    return jsonify(message_obj), status_code[m
