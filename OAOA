[32m+[m
[32m+[m[32mimport mysql.connector[m
[32m+[m[32mimport os[m
[32m+[m
[32m+[m[32m# Get MySQL credentials from environment variables[m
[32m+[m[32mMYSQL_HOST = os.getenv("MYSQL_HOST", "127.0.0.1")[m
[32m+[m[32mMYSQL_USER = os.getenv("MYSQL_USER", "root")[m
[32m+[m[32mMYSQL_PASSWORD = os.getenv("MYSQL_PASSWORD", "root")[m
[32m+[m[32mMYSQL_DB = os.getenv("MYSQL_DB", "managementdb")[m
[32m+[m
[32m+[m[32m# Connect to MySQL[m
[32m+[m[32mdef get_db_connection():[m
[32m+[m[32m    return mysql.connector.connect([m
[32m+[m[32m        host=MYSQL_HOST,[m
[32m+[m[32m        user=MYSQL_USER,[m
[32m+[m[32m        password=MYSQL_PASSWORD,[m
[32m+[m[32m        database=MYSQL_DB[m
[32m+[m[32m    )[m
